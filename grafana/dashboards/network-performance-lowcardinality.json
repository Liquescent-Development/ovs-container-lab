      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 3
      },
      "id": 9,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": false,
            "displayName": "Tenant"
          }
        ]
      },
      "pluginVersion": "7.5.0",
      "targets": [
        {
          "groupBy": [],
          "measurement": "ovs_interface_metadata",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT \n  last(\"tenant_id\") AS \"Tenant\",\n  last(\"container_id\") AS \"Container\",\n  last(\"network_id\") AS \"Network\"\nFROM \"ovs_interface_metadata\" \nWHERE $timeFilter \nGROUP BY \"uuid\"",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "table",
          "select": [
            [
              {
                "params": ["tenant_id"],
                "type": "field"
              }
            ]
          ],
          "tags": []
        }
      ],
      "title": "Interface Metadata Lookup",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "Container": "Container ID",
              "Network": "Network ID",
              "Tenant": "Tenant ID",
              "uuid": "UUID"
            }
          }
        }
      ],
      "type": "table"
    },
    {
