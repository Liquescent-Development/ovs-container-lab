        "sortBy": [
          {
            "desc": true,
            "displayName": "RX Rate"
          }
        ]
      },
      "pluginVersion": "7.5.0",
      "targets": [
        {
          "groupBy": [],
          "measurement": "ovs_ovs_interface_rx_packets_total",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT \n  non_negative_derivative(last(\"counter\"), 1s) AS \"RX Rate\"\nFROM \"ovs_ovs_interface_rx_packets_total\" \nWHERE $timeFilter AND time > now() - 1m\nGROUP BY \"uuid\"",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "table",
          "select": [],
          "tags": []
        },
        {
          "groupBy": [],
          "measurement": "ovs_ovs_interface_tx_packets_total",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT \n  non_negative_derivative(last(\"counter\"), 1s) AS \"TX Rate\"\nFROM \"ovs_ovs_interface_tx_packets_total\" \nWHERE $timeFilter AND time > now() - 1m\nGROUP BY \"uuid\"",
          "rawQuery": true,
          "refId": "B",
          "resultFormat": "table",
          "select": [],
          "tags": []
        }
      ],
      "title": "Current Interface Rates",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {
              "uuid": 0,
              "RX Rate": 1,
              "TX Rate": 2
            },
            "renameByName": {
              "uuid": "UUID"
            }
          }
        }
      ],
      "type": "table"
    },
